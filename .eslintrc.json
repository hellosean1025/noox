{
  "env": {
      "node": true,
      "commonjs": true,
      "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": 2017
  },
  "extends": ["eslint:recommended"],
  "rules": {
      "indent": ["off", 2],
      "comma-dangle": ["error", "never"],
      "no-console": ["off"],
      "no-undef": ["off"]
  }
}